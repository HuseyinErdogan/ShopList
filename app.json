{
  "expo": {
    "name": "Dobydoby: My Shopping List",
    "slug": "dobydoby-my-shopping-list",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appstore.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/itunes2x.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff93cd"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.huseyinerdogan.dobydoby",
      "buildNumber": "1.0.0",
      "icon": "./assets/appstore.png",
      "requireFullScreen": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.huseyinerdogan.dobydoby",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/playstore_icon.png",
        "backgroundColor": "#ff93cd"
      },
      "icon": "./assets/playstore-icon.png",
      "googleServicesFile": "./google-services.json"
    },
    "extra": {
      "eas": {
        "projectId": "803e46c6-eea4-4bae-81d6-7f7bd80206e1"
      }
    },
    "plugins": [
      [
        "expo-custom-assets",
        {
          "assetsPaths": ["./assets", "./src/assets"]
        }
      ],
      "@react-native-google-signin/google-signin",
      [
        "@react-native-firebase/app",
        {
          "ios_google_services_file": "./GoogleService-Info.plist",
          "android_google_services_file": "./google-services.json"
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/803e46c6-eea4-4bae-81d6-7f7bd80206e1"
    }
  }
}
